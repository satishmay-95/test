import arcpy
import os
import datetime
import xlwt

# Set workspace and output folder path
arcpy.env.workspace = "D:/GIS/$$ FLANCE $$/2.Surankoot/2.Shapefiles/Data model SHP"
output_folder = "Shapefiles_Dump"
excel_file = "Shapefiles_Dump.xls"

# Create output folder if it doesn't already exist
if not os.path.exists(os.path.join(arcpy.env.workspace, output_folder)):
    os.mkdir(os.path.join(arcpy.env.workspace, output_folder))

# Define shapefile names and data types
shapefile_list = [("AOI.shp", "POLYGON"),
                  ("MUN_AREA.shp", "POLYGON"),
                  ("ZONE.shp", "POLYGON"),
                  ("WARD.shp", "POLYGON"),
