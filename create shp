        arcpy.AddField_management(output_shapefile, "MUN_CODE", "TEXT" , field_length=3)
        arcpy.AddField_management(output_shapefile, "MUN_NAM", "TEXT" , field_length=8)
        arcpy.AddField_management(output_shapefile, "T_AR", "DOUBLE" , field_precision=4, field_scale=2)
        arcpy.AddField_management(output_shapefile, "T_POP", "LONG" , field_precision=8, field_scale=0)
        arcpy.AddField_management(output_shapefile, "T_WARDS", "LONG" , field_precision=3, field_scale=0)
        arcpy.AddField_management(output_shapefile, "TIME_ST", "DATE")


