# Loop through shapefile_list and create shapefiles with specified data types and fields
for shapefile_name, shapefile_type in shapefile_list:
    output_shapefile = os.path.join(output_folder, shapefile_name)

    # Check if the output shapefile already exists
    if os.path.exists(output_shapefile):
        print("{} already exists. Skipping...".format(output_shapefile))
        continue

    arcpy.CreateFeatureclass_management(arcpy.env.workspace, output_shapefile, shapefile_type)

        # Add fields with specific lengths to each shapefile
    if shapefile_name == "AOI.shp":
        arcpy.AddField_management(output_shapefile, "AOI_ID", "TEXT", field_length=5)
        arcpy.AddField_management(output_shapefile, "DIST_CODE", "TEXT", field_length=5)
        arcpy.AddField_management(output_shapefile, "CLASS", "TEXT" , field_length=5)
        arcpy.AddField_management(output_shapefile, "SUB_CLASS", "TEXT" , field_length=5)
        arcpy.AddField_management(output_shapefile, "TN_NAME", "TEXT" , field_length=20)
        arcpy.AddField_management(output_shapefile, "AOI_AREA", "DOUBLE" , field_precision=3, field_scale=2)
        arcpy.AddField_management(output_shapefile, "MUN_AREA", "DOUBLE" , field_precision=3, field_scale=2)
        arcpy.AddField_management(output_shapefile, "TIME_ST", "DATE")
	

    elif shapefile_name == "MUN_AREA.shp":
